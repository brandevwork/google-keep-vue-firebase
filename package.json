    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-vue": "^5.2.3",
    "husky": "^3.0.0",
    "lint-staged": "^9.2.0",
    "node-sass": "^4.12.0",
    "prettier": "^1.18.2",
